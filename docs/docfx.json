{
    "metadata": [
      {
        "src": [
          {
            "files": [ "AutoTensor.csproj" ],
            "exclude": [ "**/obj/**", "**/bin/**", "_site/**" ],
            "cwd": "../src/AutoTensor"
          }
        ],
        "dest": "obj/api",
        "properties": { "TargetFramework": "netcoreapp2.0" } 
      }
    ],
    "build": {
      "content": [
        {
          "files": [ "**/*.yml" ],
          "cwd": "obj/api",
          "dest": "api"
        },
        {
          "files": [ "toc.yml", "*.md" ],
          "exclude": [ "obj/**" ]
        },
        {
          "files": [
            "getting-started/*.md",
            "about/*.md",
            "docs/*.md",
            "api/*.md",
            "support/*.md"
          ]
        },
        {
          "files": [
              "toc.yml",
              "getting-started/toc.yml",
              "about/toc.yml",
              "docs/toc.yml",
              "api/toc.yml",
              "support/toc.yml"
            ]
          }
      ],
      "resource": [
        {
          "files": [ "images/**" ],
          "exclude": [ "obj/**" ]
        }
      ],
      "globalMetadataFiles": [ "version.json" ],
      "fileMetadataFiles": [],
      "globalMetadata": {
        "_gitContribute": {
        "repo": "https://github.com/sethjuarez/autotensor",
        "branch": "dev"
      },
      "template": [ "default" ]
    }
  }
}